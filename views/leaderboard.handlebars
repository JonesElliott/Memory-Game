<section>
    <header>
        <link rel="stylesheet" href="/assets/css/leaderboard.css" type="text/css" />
            <img src="/assets/img/icon.png" alt="logo" />
            <h1>Leaderboard</h1>
    </header>
        <div class="controls">
            <button id="home-button">Home</button>
            <button id="play-button">Play Game</button>
        </div>

    <div>
        <div>
            <h3>Fastest memory players we've seen!</h3>
        </div>
        <table>
            <tr>
                <th>Username</th>
                <th>Time</th>
            </tr>
            {{#each users}}
            {{> user-partial username=username time=time}}
            {{/each}}
        </table>
    </div>
</section>